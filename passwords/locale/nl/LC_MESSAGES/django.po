#: validators.py:31
#, python-format
msgid "Invalid Length (%s)"
msgstr "Incorrecte lengte (%s)"

#: validators.py:41
#, python-format
msgid "Must be %s characters or more"
msgstr "Moet minimaal %s karakters zijn"

#: validators.py:45
#, python-format
msgid "Must be %s characters or less"
msgstr "Moet gelijk of minder dan %s karakters zijn"

#: validators.py:49
#, python-format
msgid "Must be more complex (%s)"
msgstr "Moet meer complex zijn (%s)"

#: validators.py:77
#, python-format
msgid "Must contain %(UPPER)s or more unique uppercase characters"
msgstr "Moet %(UPPER)s of meer unieke hoofdletters bevatten"

#: validators.py:81
#, python-format
msgid "Must contain %(LOWER)s or more unique lowercase characters"
msgstr "Moet %(LOWER)s of meer kleine letters bevatten"

#: validators.py:85
#, python-format
msgid "Must contain %(DIGITS)s or more unique digits"
msgstr "Moet %(DIGITS)s of meer unieke getallen bevatten"

#: validators.py:89
#, python-format
msgid "Must contain %(PUNCTUATION)s or more unique punctuation characters"
msgstr "Moet %(PUNCTUATION)s of meer unieke speciale tekens bevatten"

#: validators.py:93
#, python-format
msgid "Must contain %(SPECIAL)s or more unique special characters"
msgstr "Moet %(SPECIAL)s of meer unieke speciale tekens bevatten"

#: validators.py:97
#, python-format
msgid "Must contain %(WORDS)s or more unique words"
msgstr "Moet %(WORDS)s of meer unieke woorden bevatten"

#: validators.py:102
#, python-format
msgid "Too Similar to [%(haystacks)s]"
msgstr "Lijkt teveel op [%(haystacks)s]"

#: validators.py:138
msgid "Based on a dictionary word"
msgstr "Gebaseerd op een woord uit het woordenboek"

#: validators.py:154
msgid "Based on a common sequence of characters"
msgstr "Gebaseerd op een veel voorkomende reeks van tekens"
